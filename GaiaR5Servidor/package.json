{
  "name": "gaiar5servidor",
  "version": "1.0.0",
  "description": "Servidor del sistema GaiaR5",
  "main": "src/server.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node --inspect=5858 -r ts-node/register ./src/server.ts",
    "start:watch": "nodemon",
    "build": "tsc"
  },
  "author": "Jindrax",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.8",
    "@types/node": "^14.11.2",
    "nodemon": "^2.0.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  },
  "dependencies": {
    "@googlemaps/google-maps-services-js": "^3.1.13",
    "@types/cors": "^2.8.7",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "neo4j-driver": "^4.1.2"
  }
}
